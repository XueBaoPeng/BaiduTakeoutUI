<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
    </style>

    <style name="MyPageButton">
        <item name="android:background">#FFFFFF</item>
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@drawable/mypage_button_right</item>
    </style>

    <style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/custem_ratingbar</item>
        <item name="android:minHeight">10dp</item>
        <item name="android:maxHeight">10dp</item>
        <item name="android:isIndicator">true</item>
        <item name="android:numStars">5</item>
    </style>

    <style name="ShopMarkTextView">
        <item name="android:background">@drawable/shop_marking</item>
        <item name="android:textColor">#1E90FF</item>
        <item name="android:gravity">center</item>
        <item name="android:isIndicator">true</item>
    </style>
</resources>
